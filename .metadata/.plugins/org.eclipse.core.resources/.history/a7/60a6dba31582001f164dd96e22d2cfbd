package Lotto;

import java.util.Scanner;
import java.util.ArrayList;

public class LottoController {
	private int tryArr[] = new int[6];
	
	public void passLottoNum(Scanner scan) { // 수동
		System.out.println("6자리 숫자를 입력하세요");
		for(int i=0; i<tryArr.length; i++) {
			tryArr[i] = scan.nextInt();
			System.out.print(tryArr[i]+ " ");
		}
		System.out.println();
	}

