package day05;

public class Product {
	private String name;
	private int price;	
	
	public Product() {}
	
	public Product(String name, int price) {
		this.name = name;
		this.price = price;
	}
	
	public void insertProduct(int cnt, String name, int price) {
		this.name = name;
		this.price = price;
	}
	@Override
	public String toString() {
		return "상품명 : " + name + ", 가격 :" + price;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	
	public int getCnt() {
		return cnt;
	}
	public void setCnt(int cnt) {
		this.cnt = cnt;
		
	}
}
