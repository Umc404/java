package day01;

public class Test04 {

	public static void main(String[] args) {
		// 배열 : 같은 타입, 같은 의미를 가지는 변수들의 집합 (참조변수)
		// 타입 배열명[] = new int[개수];
		String s = new String("가나다");
		// String은 워낙 자주 사용하는 참조변수 => 일반 지역변수처럼 사용
		String s1 = "asdf";
		System.out.println(s);
		System.out.println(s1);
	}

}
